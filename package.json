{
  "name": "miragejs-open-api",
  "version": "0.0.1",
  "description": "Generate miragejs code based on an OpenAPI Specification",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src",
    "format": "prettier src/**/*.js --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": ["npm run format", "npm run lint", "git add"]
  },
  "bin": {
    "miragejs-open-api": "./index.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dot": "^1.1.3",
    "fs-extra": "^8.1.0",
    "json-schema-faker": "^0.5.0-rc23",
    "prettier": "^1.19.1",
    "swagger-parser": "^8.0.1",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0"
  }
}
